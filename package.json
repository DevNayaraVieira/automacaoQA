        "istanbul": "^0.4.5",
        "jest": "^29.7.0",
        "json-schema-validator": "^1.0.1",
        "jsonschema": "^1.4.1",
        "lighthouse": "^11.3.0",
        "lint-staged": "^15.2.0",
